<div  id="search-wrapper">
  <form method="GET">
    <label class="govuk-label" for="event-name">Search</label>
    <input class="govuk-search-input search-box" id="search" name="search" type="text" value="{{query}}" autocomplete="off" placeholder="Search the content hub">
    <button type="submit" class="govuk-button search-button">
      Search
    </button>
  </form>
</div>

    <script src="/public/javascript/all.js"></script>
    <script src="/public/jquery.min.js"></script>
    <script src="/public/javascript/vendors/typeahead.bundle.min.js"></script>
    <script src="/public/javascript/search.js"></script>
</div>  

